    "roboto-fontface": "*",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue3-google-login": "^2.0.12",
    "vuetify": "^3.0.0-beta.11",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.59",
    "@types/webfontloader": "^1.6.34",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
