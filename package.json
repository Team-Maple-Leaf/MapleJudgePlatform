    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "~1.32",
    "sass-loader": "^13.0.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.23.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^0.40.7"
